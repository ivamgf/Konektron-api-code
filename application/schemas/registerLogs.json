{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://konektron.local/api/logs.json#",
    "type": "object",
    "properties": {
        "id_users": {
            "type": ["integer", "null"],
            "minimum": 1
        },
        "id_providers": {
            "type": ["integer", "null"],
            "minimum": 1
        },
        "id_admin": {
            "type": ["integer", "null"],
            "minimum": 1
        },
        "lo_type": {
            "type": "string",
            "minLength": 1,
            "maxLength": 100
        },
        "lo_description": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255
        }
    },
    "required": ["lo_type", "lo_description"],
    "additionalProperties": false
}

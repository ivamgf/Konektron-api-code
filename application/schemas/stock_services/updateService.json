{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://konektron.local/api/service.json#",
    "type": "object",
    "properties": {
        "id_providers": {
            "type": "integer",
            "minimum": 1
        },
        "sv_name": {
            "type": "string",
            "minLength": 1,
            "maxLength": 200
        },
        "sv_codigo": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255
        },
        "sv_description": {
            "type": "string",
            "minLength": 1,
            "maxLength": 500
        },
        "sv_image": {
            "type": "string",
            "maxLength": 200
        },
        "sv_oldprice": {
            "type": "number",
            "minLength": 1,
            "maxLength": 10
        },
        "sv_bestprice": {
            "type": "number",
            "minLength": 1,
            "maxLength": 10
        },
        "sv_discount": {
            "type": "number",
            "minLength": 1,
            "maxLength": 10
        },
        "sv_infotec": {
            "type": "string",
            "minLength": 1,
            "maxLength": 500
        },
        "sv_infocomp": {
            "type": "string",
            "minLength": 1,
            "maxLength": 500
        },
        "sv_ean": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255
        },
        "sv_gtin": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255
        },
        "sv_tag": {
            "type": "array",
            "uniqueItems": true,
            "contains": {
                "type": "string"
            }
        }
    },
    "additionalProperties": false
}
